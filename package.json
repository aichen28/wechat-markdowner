{
  "name": "wechat-markdowner",
  "version": "1.0.0",
  "description": "A online markdown converter specially for Wechat Public formatting.",
  "main": "index.js",
  "scripts": {
    "server": "NODE_ENV=development && webpack-dev-server --host 0.0.0.0 --port 5014 --inline --hot --progress --config ./webpack.config.js",
    "build": "NODE_ENV=production && webpack --progress --profile --color --display-modules --display-chunks --config ./webpack.production.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ironmaxtory/wechat-markdowner.git"
  },
  "keywords": [
    "wechat",
    "markdown",
    "format"
  ],
  "author": "Hades <ironmax@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ironmaxtory/wechat-markdowner/issues"
  },
  "homepage": "https://github.com/ironmaxtory/wechat-markdowner",
  "dependencies": {
    "copy-webpack-plugin": "~4.0.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.30.1",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.9.7"
  }
}
